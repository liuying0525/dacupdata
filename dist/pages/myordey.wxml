
        <view class="">
               
  <block  wx:if="{{$userinfo_alert$zt}}">
    <view class="wrap">
      <view class="alert">
        <view class="alert_text">{{$userinfo_alert$text_}}</view>
        <button class="alert_btn" open-type="getUserInfo" bindgetuserinfo="$userinfo_alert$bindGetUserInfo">确定</button>
      </view>
    </view>
  </block>

  
  <block  wx:if="{{$alert_l$zt}}">
    <view class="wrap">
      <view class="alert">
        <view class="alert_text">{{$alert_l$text_}}</view>
        <button class="alert_btn" open-type="getPhoneNumber" bindgetphonenumber="$alert_l$bindalertInfo">确定</button>
      </view>
    </view>
  </block>

      <view class="kong"  wx:if="{{text.length === 0}}">
      <image class="kong-image1" src="{{url_link?url_link + 'kong.png':''}}" mode="widthFix"/>
      <image class="kong-image2" src="{{url_link?url_link + 'guanguang.png':''}}" mode="widthFix" bindtap="gg"/>
    </view>
            <view class="container">
 <view  wx:if="{{text.length > 0}}" class="touch-item {{footIndex == index ? 'touch-move-active' : ''}}" data-index="{{index}}" bindtouchstart="touchstart" bindtouchmove="touchmove" wx:for="{{text}}" wx:key="key">
    <view class="successfulorder content" bindtap="to_xq" data-wpyto_xq-a="{{item}}" data-wpyto_xq-b="{{item.collectiontype}}">
        <view class="successfulorder-xinxi">
              <view class="successfulorder-tupian">
                <view class="brand-list-textone1">{{item.collectiontype === 0 ? '新车贷' : '直租'}}</view>
                <image class="about-img-img" src="{{item.carmodel.thumbnailimgurl}}" mode="aspectFill"/>
              </view>
              <view class="successfulorder-xiangqing">
                <view class="successfulorder-xiangqing-one">{{item.carmodel.name}}</view>
                <view class="successfulorder-xiangqing-tow" wx:if="{{item.collectiontype === 1 ?false:true}}">首付<text>{{item.downpayment}}</text>万  月供<text>{{item.msup}}</text>元</view>
                <view class="successfulorder-xiangqing-tow" wx:if="{{item.collectiontype === 0 ?false:true}}">首付<text>{{item.downpayment}}</text>万  月租<text>{{item.msup}}</text>元</view>
               <view class="tiexu" wx:if="{{item.financial_product.interest === 1 ?true:false}}">{{item.financial_product.interest === 1 ? '0息' : ''}}</view>
               <view class="tiexu" wx:if="{{item.financial_product.interest === 2 ?true:false}}">{{item.financial_product.interest === 2 ? '低息' : ''}}</view>
              </view>

        </view>
        <!-- <view class='scheme' wx:if="{{item.collectiontype === 0}}"> -->
        <view class="scheme">
            <view class="scheme-one">产品金融方案：
                <text wx:if="{{item.financial_product.financial_name!=''?false:true}}">{{item.financial_product.financial_name}}</text>
                <text wx:if="{{item.financial_product.line_pro_name!=''?true:false}}">{{item.financial_product.line_pro_name}}</text>
              </view>
            <view class="scheme-tow" wx:if="{{!item.financial_product.activityJO.name?false:true}}"><image class="liwu" src="{{url_link?url_link + 'lw@2x.png':''}}"/>{{item.financial_product.activityJO.name}}</view>
        </view>
    </view>

      <view class="del" bindtap="del" data-index="{{index}}" data-wpydel-a="{{item.collectionid}}">删除</view>
 </view>
</view>
        </view>
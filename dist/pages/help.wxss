.testresult {
  padding: 0 40rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.bzzx {
  width: 90%;
  margin-left: 25rpx;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
  font-family: 'PingFangSC-Regular';
  font-size: 28rpx;
}
.bzzx-view1 {
  display: flex;
  font-size: 28rpx;
}
.view1 {
  color: #5c45c3;
  margin-bottom: 21rpx;
}
.view2 {
  color: #737373;
}
.bzzx-w {
  margin-left: 30rpx;
}
.bzzx-lxwm {
  width: 100%;
  position: fixed;
  right: 10rpx;
  bottom: 140rpx;
}
.bzzx-lxwm-yuan {
  width: 68rpx;
  height: 68rpx;
  text-align: center;
  background: #00c7ae;
  font-size: 24rpx;
  border-radius: 50rpx;
  padding: 13rpx;
  position: absolute;
  left: 83%;
  color: #fff;
}
.bzzx-lxwm-yuan-2 {
  width: 68rpx;
}
.types {
  width: 100%;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 20%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  display: flex;
  margin-bottom: 20rpx;
}
.type {
  border-bottom: 8rpx solid #fff;
  flex: 1;
  text-align: center;
  margin: 0 50rpx;
}
.activeType {
  color: #6044ca;
  border-color: #6044ca;
}
.dqBox,
.dhBox {
  padding: 20rpx;
  -webkit-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
}
.textTitle {
  color: red;
  font-size: 28rpx;
  text-indent: 25rpx;
}
.textInfo {
  color: #737373;
}

.right_sidebar {
  display: block;
  position: fixed;
  top: 0;
  height: 100%;
  right: 0;
  width: 70%;
  z-index: 999999;
  transition: all 0.5s;
  -webkit-transition: all 0.5s;
}
.right_sidebar_false {
  display: block;
  position: fixed;
  top: 0;
  height: 100%;
  right: -100%;
  width: 100%;
  z-index: 999999;
  transition: all 0.5s;
  -webkit-transition: all 0.5s;
}
.sidebar {

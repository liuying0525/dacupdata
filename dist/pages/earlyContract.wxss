.earlyContractS {
  padding: 46rpx 50rpx;
}
.earlyContractS view,
.earlyContractS input {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.earlyContractS .earlyContractSt {
  padding: 0 58rpx 30rpx;
  margin-bottom: 54rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
}
.earlyContractS .earlyContractSt .title {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #000;
  text-align: center;
  margin-bottom: 10rpx;
}
.earlyContractS .earlyContractSt .textItem {
  position: relative;
  width: 100%;
  height: 56rpx;
  font-size: 26rpx;
  margin-bottom: 44rpx;
}
.earlyContractS .earlyContractSt .textItem.radioItem {
  margin-bottom: 15rpx;
}
.earlyContractS .earlyContractSt .textItem text {
  float: left;
  line-height: 56rpx;
}
.earlyContractS .earlyContractSt .textItem .early {
  color: #6044cb;
  position: relative;
}
.earlyContractS .earlyContractSt .textItem .earlyHelp {
  position: absolute;
  display: block;
  right: -30rpx;
  top: -10rpx;
  line-height: 26rpx;
  width: 26rpx;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  text-align: center;
  border: 1px solid #6044cb;
  font-size: 20rpx;
}
.earlyContractS .earlyContractSt .textItem .earlyTime {
  height: 56rpx;
  padding: 0 20rpx;
  margin-left: 40rpx;
}
.earlyContractS .earlyContractSt .textItem input {
  float: right;
  height: 56rpx;
  border: 1px solid #ddd;
  width: 330rpx;
  padding-left: 30rpx;
  font-size: 24rpx;
}
.earlyContractS .earlyContractSt .textItem .errorInfo {
  position: absolute;
  right: 20rpx;
  top: 66rpx;
  font-size: 22rpx;
  color: #6044cb;
  text-align: right;
  border-bottom: 1px solid #6044cb;
}
.earlyContractS .earlyContractSt .textItem.cardblack {
  background-color: #f2f4f8;
  position: relative;
  line-height: 35rpx;
  padding: 22.94rpx 36.22rpx;
  height: auto;
  font-size: 19.32rpx;
  color: #636466;
}
.earlyContractS .earlyContractSt .textItem.cardblack::after {
  content: '';
  width: 0;
  height: 0;
  border-right: 30rpx solid transparent;
  border-left: 30rpx solid transparent;
  border-bottom: 30rpx solid #f2f4f8;
  position: absolute;
  top: -20rpx;
}
.earlyContractS .earlyContractSt .textItem.cardblack text {
  float: none;
  line-height: 35rpx;
}
.earlyContractS .earlyContractSt .textItem.cardblack.cardblack1::after {
  left: 20%;
}
.earlyContractS .earlyContractSt .textItem.cardblack.cardblack2::after {
  right: 10%;
}
.earlyContractS .earlyContractSt .textItemInfos {
  position: absolute;
  line-height: 56rpx;
  border: 1px solid #e5e5e5;
  border-bottom: none;
  border-top: none;
  width: 370rpx;
  font-size: 24rpx;
  right: 0;
  top: 56rpx;
  z-index: 100;
}
.earlyContractS .earlyContractSt .textItemInfos .textItemInfo {
  height: 56rpx;
  border-bottom: 1px solid #e5e5e5;
  font-size: 20rpx;
  color: #393939;
  background: #fff;
  text-align: center;
}
.earlyContractS .earlyBtn {
  width: 546rpx;
  height: 74rpx;
  line-height: 74rpx;
  color: #fff;
  background: #6044cb;
  font-size: 30rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  border-radius: 10rpx;
  margin: 86rpx auto 0;
  text-align: center;
}
.earlyContractS .earlyNoBtn {
  width: 546rpx;
  height: 74rpx;
  line-height: 74rpx;
  color: #fff;
  background: #6044cb;
  font-size: 30rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  border-radius: 10rpx;
  margin: 86rpx auto 0;
  text-align: center;
  background: #e5e5e5;
  color: #000;
}
.earlyContractS .earlyInfo {
  float: right;
  width: 330rpx;
  font-size: 22rpx;
  color: #595959;
}
.earlyContractS .getDetail {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #6044cb;
  text-align: center;
}
.earlyContractS .radio-group {
  font-size: 24.15rpx;
  display: flex;
  justify-content: space-between;
}
.earlyContractS .radio-group radio {
  padding-right: 25.35rpx;
}

.secRegister .header {
  margin: 62rpx 0 52rpx;
}
.secRegister .header image {
  display: block;
  height: 125rpx;
  width: 125rpx;
  margin: 0 auto 46rpx;
}
.secRegister .header .warnInfo {
  font-size: 20rpx;
  color: #666;
  text-align: center;
}
.secRegister .inputBox {
  height: 70rpx;
  width: 100%;
  padding: 0 40rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  position: relative;
  margin-bottom: 68rpx;
}
.secRegister .inputBox .errorInfo {
  position: absolute;
  color: #ff0202;
  font-size: 22rpx;
  text-align: right;
  right: 40rpx;
  bottom: -40rpx;
}
.secRegister .inputBox input {
  display: block;
  margin: 0 auto;
  height: 68rpx;
  border: 1px solid #5c45c3;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  border-radius: 10rpx;
  padding: 0 30rpx;
  font-size: 26rpx;
}
.secRegister .inputBox .codeInput {
  display: block;
  height: 68rpx;
  border: 1px solid #5c45c3;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  border-radius: 10rpx;
  padding: 0 30rpx;
  font-size: 26rpx;
  width: 328rpx;
  float: left;
}
.secRegister .inputBox .codeBtn {
  display: block;
  height: 68rpx;
  border: 1px solid #5c45c3;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  border-radius: 10rpx;
  padding: 0 30rpx;
  font-size: 26rpx;
  width: 328rpx;
  float: left;
  float: right;
  width: 230rpx;
  color: #fff;
  background: #5c45c3;
  padding: 0;
  line-height: 68rpx;
  text-align: center;
}
.secRegister .inputBox .codeBtnNo {
  background: #e5e5e5;
  color: #000;
  border-color: #e5e5e5;
}
.secRegister .inputBox .button {
  margin: 0 auto;
  height: 68rpx;
  line-height: 68rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  border-radius: 10rpx;
  background: #5c45c3;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
}
.secRegister .forget {
  float: left;
  font-size: 26rpx;
  color: #5c45c3;
  border-bottom: 1px solid #5c45c3;
}
.secRegister .register {
  float: left;
  font-size: 26rpx;
  color: #5c45c3;
  border-bottom: 1px solid #5c45c3;
  float: right;
}

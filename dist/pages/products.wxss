@import "./../components/substitution.wxss";
/**index.wxss**/
page {
  width: 100%;
  margin: 0 auto;
  background: #eee;
}
.brand {
  width: 100%;
  background: #5c45c3;
  padding-top: 10rpx;
  padding-bottom: 10rpx;

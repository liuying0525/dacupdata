.earlyList {
  padding: 130rpx 50rpx 46rpx;
}
.earlyList view,
.earlyList input {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.earlyList .earlyTime {
  height: 85rpx;
  line-height: 85rpx;
  font-size: 26rpx;
  padding: 0 58rpx;
  border-bottom: 1px solid #e4e6eb;
}
.earlyList .titles {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  text-align: center;
  background: #fff;
  padding: 40rpx 0 10rpx;
  z-index: 100;
}
.earlyList .title {
  font-size: 30rpx;
  color: #4b4b4b;
  padding-bottom: 12rpx;
  border-bottom: 8rpx solid #fff;
  margin: 0 auto;
  max-width: 120rpx;
  flex: 1.0;
}
.earlyList .activeTitle {
  color: #6044ca;
  border-color: #6044ca;
}
.earlyList .earlyBox {
  -webkit-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  margin-bottom: 34rpx;
}
.earlyList .earlyItem {
  position: relative;
  padding: 38rpx 58rpx 50rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.earlyList .earlyItem .textItem {
  width: 100%;
  font-size: 26rpx;
  margin-bottom: 44rpx;
}
.earlyList .earlyItem .changeBtn {
  height: 40rpx;
  line-height: 40rpx;
  color: #fff;
  background: #6044cb;
  text-align: center;
  -webkit-border-radius: 6rpx;
  -moz-border-radius: 6rpx;
  border-radius: 6rpx;
  margin-right: 66rpx;
  padding: 4rpx 10rpx;
  border: 1px solid #6044cb;
}
.earlyList .earlyItem .cancelBtn {
  height: 40rpx;
  line-height: 40rpx;
  color: #fff;
  background: #6044cb;
  text-align: center;
  -webkit-border-radius: 6rpx;
  -moz-border-radius: 6rpx;
  border-radius: 6rpx;
  margin-right: 66rpx;
  padding: 4rpx 10rpx;
  border: 1px solid #6044cb;
  color: #6044cb;
  background: #fff;
}
.earlyList .earlyItem .itemState {
  position: absolute;
  right: 32rpx;
  bottom: 90rpx;
}
.earlyList .earlyItem .itemState image {
  height: 105rpx ;
  width: 139rpx;
}
.earlyList .cancelReason {
  background: #f2f4f8;
  padding: 38rpx 58rpx;
}
.earlyList .cancelReason .textItem {
  width: 100%;
  font-size: 26rpx;
  margin-bottom: 0rpx;
}
.createOne {
  height: 82rpx;
  line-height: 82rpx;
  position: fixed;
  bottom: 0;
  color: #fff;
  background: #6144cb;
  font-size: 30rpx;
  width: 100%;
  text-align: center;
}
.createOne .addIcon {
  color: #fff;
  font-size: 40rpx;
  font-weight: bolder;
  padding-right: 10rpx;
}
.createOnePro {
  height: 112rpx;
  line-height: 82rpx;
  padding: 0 0 30rpx;
}
.noData {
  width: 500rpx;
  margin: 300rpx auto;
}
.noData image {
  display: block;
  height: 252rpx;
  width: 309rpx;
  margin: 0 auto;
}
.noData .textInfo {
  text-align: center;
  font-size: 36rpx;
  line-height: 120rpx;
}

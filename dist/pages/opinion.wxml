
     <view class="testresult">
    
  <block  wx:if="{{$userinfo_alert$zt}}">
    <view class="wrap">
      <view class="alert">
        <view class="alert_text">{{$userinfo_alert$text_}}</view>
        <button class="alert_btn" open-type="getUserInfo" bindgetuserinfo="$userinfo_alert$bindGetUserInfo">确定</button>
      </view>
    </view>
  </block>

    
  <block  wx:if="{{$alert_l$zt}}">
    <view class="wrap">
      <view class="alert">
        <view class="alert_text">{{$alert_l$text_}}</view>
        <button class="alert_btn" open-type="getPhoneNumber" bindgetphonenumber="$alert_l$bindalertInfo">确定</button>
      </view>
    </view>
  </block>

     <view class="opinion">
        <form bindsubmit="bindFormSubmit">
            <textarea placeholder="您留下的每个字都将帮助我们改善产品，期待您的反馈！" name="textarea" class="opinion-textarea" maxlength="200"/>
            <button form-type="submit" class="opinion-button"> 提交 </button>
        </form>
    </view>
    </view>
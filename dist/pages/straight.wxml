      <view class="index_gps" bindtap="$search$dingwei">
        <image class="index_gps_img" src="{{$search$url_link?$search$url_link + 'index_gps_icon.png':''}}" mode="widthFix"/>
        {{$search$gps_wz}}
      </view>
    </view>
  </view>

    <view class="brand-condition"/>
    <!-- 头部车贷流程 -->
    <view class="straight-liucheng_center">
      <view class="straight-liucheng">

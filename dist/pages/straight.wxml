            <image class="joio-img1" src="{{url_link?url_link+'jiantou.png':''}}" mode="widthFix"/>
          </view>
          <view class="straight-li">
            <image class="joio-img1" src="{{url_link?url_link+'goutong.png':''}}" mode="widthFix"/>
            <view class="joio-c">顾问沟通</view>
          </view>
          <view class="straight-li2">
            <image class="joio-img1" src="{{url_link?url_link+'jiantou.png':''}}" mode="widthFix"/>
          </view>
          <view class="straight-li">
            <image class="joio-img1" src="{{url_link?url_link+'xin.png':''}}" mode="widthFix"/>
            <view class="joio-c">信用评估</view>
          </view>
          <view class="straight-li2">
            <image class="joio-img1" src="{{url_link?url_link+'jiantou.png':''}}" mode="widthFix"/>
          </view>
          <view class="straight-li">
            <image class="joio-img1" src="{{url_link?url_link+'zhifu.png':''}}" mode="widthFix"/>
            <view class="joio-c joio-v">支付保障金提车</view>
          </view>
          <view class="straight-li2">
            <image class="joio-img1" src="{{url_link?url_link+'/jiantou.png':''}}" mode="widthFix"/>

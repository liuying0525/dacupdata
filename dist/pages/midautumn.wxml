
  <view>
    
  <block  wx:if="{{$userinfo_alert$zt}}">
    <view class="wrap">
      <view class="alert">
        <view class="alert_text">{{$userinfo_alert$text_}}</view>
        <button class="alert_btn" open-type="getUserInfo" bindgetuserinfo="$userinfo_alert$bindGetUserInfo">确定</button>
      </view>
    </view>
  </block>

    
  <block  wx:if="{{$alert_l$zt}}">
    <view class="wrap">
      <view class="alert">
        <view class="alert_text">{{$alert_l$text_}}</view>
        <button class="alert_btn" open-type="getPhoneNumber" bindgetphonenumber="$alert_l$bindalertInfo">确定</button>
      </view>
    </view>
  </block>

    <view class="one">
      <image class="widget__arrow" src="{{topimgurl}}" mode="aspectFill"/>
    </view>
    <view class="successfulorder-danhao"  wx:if="{{floorName==''?false:true}}">
      <view class="">{{floorName}}</view>
    </view>

    <view class="barand-one">
      <view class="brand-list"  wx:for="{{carmodel}}" wx:key="key">
        <view class="daxiao1 iconfont #icon-xuanze" wx:for-index="index" wx:for-item="item" wx:key="key" style="{{key==index?'color:#6044ca;border:2rpx #fff solid;':''}} ;" bindtap="changColor" data-zhi="{{item}}" data-index="{{index}}">
        </view>
        
        <view class="brand-list-img">
          <image class="widge" src="{{item.thumbnailimgurl}}" mode="widthFix">
          </image>
        </view>
        <view class="brand-list-details">
          <view class="brand-list-details1">{{item.carseriesname}}</view>
          <view class="brand-list-details2">{{item.name}}</view>
          <view class="brand-list-details4">
            <text class="brand-list-four">首付<text>{{item.downpayment/10000|Int}}</text>万</text>
            <text class="brand-list-five">月供<text>{{item.monthsuppert}}</text>元</text>
          </view>
        </view>
      </view>

    </view>
    <view class="zxx" wx:if="{{lookatmore=='0'?false:true}}" bindtap="gengd" data-wpygengd-a="{{cartype}}">
      <view class="gengd" bindtap="gengd">查看更多</view>
    </view>
    <view class="zxs">
      <view class="zxkf"/>
    </view>
    <view class="zx">
      <view class="zxkf" bindtap="zxkf_btn">提交</view>
    </view>
  </view>
.earlyList {
  padding: 130rpx 50rpx 46rpx;
}
.earlyList view,
.earlyList input {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.earlyList .earlyTime {
  height: 85rpx;
  line-height: 85rpx;
  font-size: 26rpx;
  padding: 0 58rpx;
  border-bottom: 1px solid #e4e6eb;
}
.earlyList .titles {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  text-align: center;
  background: #fff;
  padding: 40rpx 0 10rpx;
  z-index: 100;
}
.earlyList .title {
  font-size: 30rpx;
  color: #4b4b4b;
  padding-bottom: 12rpx;
  border-bottom: 8rpx solid #fff;
  margin: 0 auto;
  max-width: 120rpx;
  flex: 1.0;
}
.earlyList .activeTitle {
  color: #6044ca;
  border-color: #6044ca;
}
.earlyList .earlyBox {
  -webkit-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  box-shadow: 0 0 16rpx 6rpx rgba(0, 0, 0, 0.1);
  margin-bottom: 34rpx;
}
.earlyList .earlyItem {
  position: relative;
  padding: 38rpx 58rpx 50rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.earlyList .earlyItem .textItem {
  width: 100%;
  font-size: 24rpx;
  margin-bottom: 44rpx;
}
.earlyList .earlyItem .changeBtn {
  width: 176rpx;
  height: 46rpx;
  line-height: 46rpx;
  color: #fff;
  background: #6044cb;
  text-align: center;
  -webkit-border-radius: 6rpx;
  -moz-border-radius: 6rpx;
  border-radius: 6rpx;
  margin-right: 66rpx;
  padding: 4rpx 10rpx;
  border: 1px solid #6044cb;
}
.earlyList .earlyItem .cancelBtn {
  width: 176rpx;
  height: 46rpx;
  line-height: 46rpx;
  color: #fff;
  background: #6044cb;
  text-align: center;
  -webkit-border-radius: 6rpx;
  -moz-border-radius: 6rpx;
  border-radius: 6rpx;
  margin-right: 66rpx;
  padding: 4rpx 10rpx;
  border: 1px solid #6044cb;
  color: #6044cb;
  background: #fff;
  float: left;
  margin: 0;
}
.earlyList .earlyItem .editBtn {
  background: #6044cb;
  color: #fff;
  float: left;
}
.earlyList .earlyItem .itemState {
  position: absolute;
  right: 32rpx;
  bottom: 384rpx;
  z-index: -2;
}
.earlyList .earlyItem .itemState image {
  height: 105rpx ;
  width: 139rpx;
}
.earlyList .cancelReason {
  background: #f2f4f8;
  padding: 38rpx 58rpx;
}
.earlyList .cancelReason .textItem {
  width: 100%;
  font-size: 26rpx;
  margin-bottom: 0rpx;
}
.earlyList .reOpen {
  background: #00c7ae;
  color: #fff;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  width: 263rpx;
  border-radius: 5rpx;
  margin: 0 auto;
}
.earlyList .noOpen {
  background: #f2f4f8;
  color: #848486;
}
.createOne {
  height: 82rpx;
  line-height: 82rpx;
  position: fixed;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  bottom: 0;
  color: #fff;
  background: #6144cb;
  font-size: 30rpx;
  width: 100%;
  text-align: center;
}
.createOne .addIcon {
  color: #fff;
  font-size: 40rpx;
  font-weight: bolder;
  padding-right: 10rpx;
}
.createOnePro {
  height: 112rpx;
  line-height: 82rpx;
  padding: 0 0 30rpx;
}
.noData {
  width: 500rpx;
  margin: 300rpx auto;
}
.noData image {
  display: block;
  height: 252rpx;
  width: 309rpx;
  margin: 0 auto;
}
.noData .textInfo {
  text-align: center;
  font-size: 36rpx;
  line-height: 120rpx;
}
.transfer {
  position: relative;
  background-color: #f2f4f8;
  padding: 14.5rpx 30.79rpx;
}
.transfer::after {
  content: '';
  width: 0;
  height: 0;
  border-right: 30rpx solid transparent;
  border-left: 30rpx solid transparent;
  border-bottom: 30rpx solid #f2f4f8;
  position: absolute;
  top: -20rpx;
  left: 30%;
}
.schedule {
  background-color: #f2f4f8;
  padding: 14.5rpx 30.79rpx;
}
.schedule > view {
  line-height: 71.23rpx;
}
.conents_lab {
  font-size: 19rpx;
  color: #636466;
  display: flex;
  justify-content: space-between;
  line-height: 38.98rpx;
}
.conents_lab .textitem_left {
  display: flex;
}
.conents_lab .textitem_left text {
  display: flex;
  flex-flow: row wrap;
  flex: 1;
}
.conents_lab .textitem_copy {
  color: #5c45c3;
  display: flex;
  flex: 1 0 1;
  width: 60rpx;
  flex-flow: nowrap;
  justify-content: center;
}

.wrap {
  width: 100%;
  min-height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 999999999;
}
.alert {
  width: 70%;
  height: auto;
  padding: 15rpx 0 0rpx 0;
  background-color: #ffffff;
  text-align: center;
  position: fixed;
  left: 15%;
  top: 25%;
  z-index: 999999;
  border-radius: 8rpx;
  overflow: hidden;
}
.alert_text {
  text-align: center;
  font-size: 30rpx;
  color: #000;
  padding: 35rpx 0 35rpx 0;
  margin-bottom: 30rpx;
}
.alert_btn {
  display: block;
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #ffffff;
  background-color: #00c7ae !important;
  border-radius: 0;
}
.mengban_touming {
  width: 100%;
  min-height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.1);
  z-index: 999999;
}

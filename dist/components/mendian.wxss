  height: 60px;
}
.zxkf {
  width: 100%;
  line-height: 80rpx;
  font-size: 28rpx;
  background: #6044ca;
  color: #fff;
  text-align: center;
  position: fixed;
  bottom: 0;
}
.right_sidebar {
  display: block;
  position: fixed;
  height: 100%;
  width: 100%;
  background: #fff;
  z-index: 999999;
  transition: all 0.5s;
  -webkit-transition: all 0.5s;
  overflow: hidden;
}
.right_sidebar_false {
  display: block;
  position: fixed;
  top: 0;
  height: 100%;
  right: -100%;
  width: 100%;
  z-index: 999999;
  transition: all 0.5s;
  -webkit-transition: all 0.5s;
}
.menu-cont {

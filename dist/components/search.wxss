.index_ss_content {
  display: block;
  width: 90%;
  padding: 0 5% 0 5%;
  height: auto;
  background-color: #6044ca;
  overflow: hidden;
  position: fixed;
  z-index: 2;
}
.index_ss {
  background-color: #4a31a4;
  font-size: 28rpx;
  height: 64rpx;
  color: #ffffff;
  border-radius: 8rpx;
  width: 61%;
  padding-left: 80rpx;
  float: left;
  line-height: 64rpx;
}
.index_input_ss {
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 42rpx;
  left: 66rpx;
}
.index_gps {
  display: inline-block;
  width: auto;
  height: 64rpx;
  float: right;
  font-size: 30rpx;
  line-height: 64rpx;
  text-align: right;
  color: #fff;
  position: relative;
  padding-left: 55rpx;
}
.index_gps_img {
  width: 38rpx;
  height: 48rpx;
  position: absolute;
  top: 8rpx;
  left: 10rpx;
}
.index_ss_header {
  width: 100%;
  overflow: hidden;
  height: auto;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
}
